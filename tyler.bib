@inproceedings{arunachalamCraftingOpportunitiesEstablishing2025,
	address = {New York, NY, USA},
	series = {{SIGCSETS} 2025},
	title = {Crafting {Opportunities}: {Establishing} a {Micro}-{Internship} {Program} for {Computing} {Students}},
