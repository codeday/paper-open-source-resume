@inproceedings{fordWeDontThat2018,
	address = {Montreal QC Canada},
	title = {"{We} {Don}'t {Do} {That} {Here}": {How} {Collaborative} {Editing} with {Mentors} {Improves} {Engagement} in {Social} {Q}\&{A} {Communities}},
