	keywords = {College Students, Counseling Services, Focus Groups, Interpersonal Competence, Leadership Training, Mentors, Outreach Programs, Peer Counseling, Professional Development, Program Descriptions, Qualitative Research, School Community Programs, Service Learning, Social Cognition, Stress Management, Student Experience, Trust (Psychology), Volunteers, Youth Problems, Youth Programs},
	pages = {225--256},
}

@article{tovarRoleFacultyCounselors2015,
	title = {The {Role} of {Faculty}, {Counselors}, and {Support} {Programs} on {Latino}/a {Community} {College} {Students}’ {Success} and {Intent} to {Persist}},
