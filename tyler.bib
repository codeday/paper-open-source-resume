	year = {2017},
	keywords = {Agile adoption, Agile practices, Communication, Coordination, Daily meetings, Daily scrum, Stand up meeting, Team size, Teamwork},
	pages = {274--281},
}

@inproceedings{pries-hejeAgileDistributedProject2017,
